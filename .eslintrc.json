{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-multiple-empty-lines":"off",
        "eol-last":"off",
        "no-unused-vars": "warn",
        "no-trailing-spaces": "off"
    }
}
